{
  "name": "comics-interface",
  "homepage": "https://estebmaister.github.io/comics",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "gh-pages": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "web-vitals": "^3.4.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@svgr/webpack": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "jest": "^29.7.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5"
  },
  "overrides": {
    "@svgr/webpack": "^8.1.0",
    "react-scripts": {
      "@svgr/webpack": "^8.1.0"
    }
  },
  "scripts": {
    "dockerize": "docker build -t comic-tracker .",
    "docker": "docker run -p 5001:5001 comic-tracker",
    "chokidar": "docker run -e CHOKIDAR_USEPOLLING=true -v ${PWD}/src/:/code/src/ -p 5001:5001 comic-tracker",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "server": "python3 src/__main__.py server",
    "scrape": "python3 src/__main__.py",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test && python3 test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}